#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/degenerator@3.0.1/node_modules/degenerator/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/degenerator@3.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/escodegen@1.14.3/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/degenerator@3.0.1/node_modules/degenerator/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/degenerator@3.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
else
  exec node  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
fi
