#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules/vm2/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules/vm2/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules/vm2/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules/vm2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules/vm2/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules/vm2/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/vm2@3.9.3/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules/vm2/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules/vm2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vm2/bin/vm2" "$@"
else
  exec node  "$basedir/../vm2/bin/vm2" "$@"
fi
