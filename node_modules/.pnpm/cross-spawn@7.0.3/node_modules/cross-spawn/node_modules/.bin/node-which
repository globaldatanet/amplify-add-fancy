#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules/which/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules/which/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/cross-spawn@7.0.3/node_modules/cross-spawn/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/cross-spawn@7.0.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules/which/bin/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules/which/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/which@2.0.2/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/cross-spawn@7.0.3/node_modules/cross-spawn/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/cross-spawn@7.0.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
else
  exec node  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
fi
