#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/john/Projects/add-fancy/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/is-docker@2.2.1/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/open@7.4.2/node_modules/open/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/open@7.4.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/john/Projects/add-fancy/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/is-docker@2.2.1/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/node_modules:/Users/john/Projects/add-fancy/node_modules:/Users/john/Projects/node_modules:/Users/john/node_modules:/Users/node_modules:/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/open@7.4.2/node_modules/open/node_modules:/Users/john/Projects/add-fancy/node_modules/.pnpm/open@7.4.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
else
  exec node  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
fi
